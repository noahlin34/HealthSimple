{
  "expo": {
    "name": "HealthSimple",
    "slug": "HealthSimple",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "healthsimple",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.noahlin.HealthSimple"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.HealthSimple"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/inter/400Regular/Inter_400Regular.ttf",
            "node_modules/@expo-google-fonts/inter/700Bold/Inter_700Bold.ttf",
            "node_modules/@expo-google-fonts/league-spartan/400Regular/LeagueSpartan_400Regular.ttf",
            "node_modules/@expo-google-fonts/league-spartan/700Bold/LeagueSpartan_700Bold.ttf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "ios" : {
            "deploymentTarget": "18.0"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app accesses your photos to allow you to select images."
        }
      ],
      "expo-sqlite",
      "expo-web-browser",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
